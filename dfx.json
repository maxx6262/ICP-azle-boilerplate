{
  "canisters": {
    "message_board": {
      "type": "custom",
      "main": "src/index.ts",
      "candid": "src/index.did",
      "candid_gen": "http",
      "build": "npx azle message_board",
      "wasm": ".azle/message_board/message_board.wasm",
      "gzip": true,
      "metadata": [
        {
          "name": "candid:service",
          "path": "src/index.did"
        },
        {
          "name": "cdk:name",
          "content": "azle"
        }
      ]
    },
    "users": {
      "type": "custom",
      "main": "src/Users/user.ts",
      "candid": "src/Users/users.did",
      "candid_gen": "http",
      "build": "npx azle users",
      "wasm": ".azle/users/users.wasm",
      "gzip": true,
      "metadata": [
        {,
          "name": "candid:service",
          "path": "src/Users/users.did"
        },
        {
          "name": "cdk:name",
          "content": "azle"
        }
      ]
    }
  }
}
